<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:visibility="visible">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginBottom="@dimen/dp50"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="5dp"
            android:padding="6dp"
            android:text="@string/no_data0"
            android:textColor="@color/black"
            android:textSize="@dimen/BigTextSize"
            android:textStyle="bold" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="5dp"
            android:padding="6dp"
            android:text="@string/no_data1"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_14_text_size" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="5dp"
            android:padding="6dp"
            android:text="@string/no_data2"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_14_text_size" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="5dp"
            android:padding="6dp"
            android:text="@string/no_data3"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_14_text_size" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="5dp"
            android:padding="6dp"
            android:text="@string/no_data4"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_14_text_size" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="5dp"
            android:padding="6dp"
            android:text="@string/no_data5"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_14_text_size" />

    </LinearLayout>

</FrameLayout>